pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 t=0
 
 ship = {
  s=1,
  x=60,
  y=60,
  h=3
  p=0
 }
 bullets = {}
 enemies = {}
 
 for i=1,5 do
	 add(enemies, {
	  sp=17,
	  m_x=i*20,
	  m_y=60-i*8,
	  x=-32,
	  y=-32,
	  r=16
	 })
	end
end

function _update()
 t = t+1
 
 if(t%6 < 3) then
 	ship.sp = 1
 else
  ship.sp = 2
 end
 
 for b in all(bullets) do
  b.x+=b.dx
  b.y+=b.dy
  
  if b.x < 0
  or b.x > 128
  or b.y < 0
  or b.y > 128 then
  	del(bullets, b)
  end
 end
 
 for e in all(enemies) do
  e.x = e.r*sin(t/50) + e.m_x
  e.y = e.r*cos(t/50) + e.m_y
 end
  
 if btn(0) then ship.x-=2 end
 if btn(1) then ship.x+=2 end
 if btn(2) then ship.y-=2 end
 if btn(3) then ship.y+=2 end 
 if btnp(5) then fire() end
end

function _draw()
 cls()
 spr(ship.sp,ship.x,ship.y)
 
 for b in all(bullets) do
  spr(b.sp,b.x,b.y)
 end
 
 for e in all(enemies) do
  spr(e.sp,e.x, e.y)
 end
 
 for i=1,4 do
  if(i<=ship.h) then
   spr(33,0+6*i,0)
  else
   spr(34,0+6*i,0)
  end
 end
end

function fire()
 local b = {
  sp=0,
  x=ship.x,
  y=ship.y-3,
  dx=0,
  dy=-9
 }
 add(bullets,b)
end

